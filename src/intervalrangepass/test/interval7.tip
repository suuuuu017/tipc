main() {
  var x, y, z, t;
  x = 10;		// x = [10,10]
  y = 1;        	// y = [1,1]
  z = 0;
  while (x > 0) {
    x = x - 1;          // x = [-minf,10]
    y = y * 2;          // x = [1,pinf]
    z = z + 1;          // [0, pinf]
    t = x / 5;          // [-minf, 2
  }
  return x + y + z + t;		// [-minf,pinf]
}
